{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root":true,
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "react",
        "jsx-a11y",
        "optimize-regex",
        "sonarjs",
        "promise",
        "node"
    ],
    "rules": {
        "linebreak-style": "off"
    }
}
